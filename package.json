{
  "name": "kbh-billeder",
  "version": "0.1.0",
  "description": "Configuration/setup for the kbhbilleder.dk webapp built on top of Collections Online",
  "main": "server.js",
  "scripts": {
    "gulp": "gulp",
    "index": "node indexing.js",
    "prepublish": "npm run create-legacy-symlinks && gulp build",
    "start:dev": "concurrently \"npm run gulp\" \"npm run start:nodemon\"",
    "start:nodemon": "nodemon --watch . --watch ../collections-online --watch ../collections-online-cumulus server.js",
    "start:inspect": "kill-port --port 9229 && node --inspect=0.0.0.0:9229 server.js",
    "start:nodemon-inspect": "nodemon --delay 80ms --watch . --watch ../collections-online --watch ../collections-online-cumulus --exec 'npm run start:inspect'",
    "create-legacy-symlinks": "ln -s ../collections-online ./node_modules/collections-online && ln -s ../collections-online-cumulus ./node_modules/collections-online-cumulus"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CopenhagenCityArchives/kbh-billeder.git"
  },
  "author": "Socialsquare for the Copenhagen City Archives",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CopenhagenCityArchives/kbh-billeder/issues"
  },
  "homepage": "https://github.com/CopenhagenCityArchives/kbh-billeder#readme",
  "dependencies": {
    "agentkeepalive": "~2.0.3",
    "auth0": "^2.9.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-babili": "0.0.9",
    "babel-preset-latest": "^6.16.0",
    "babelify": "^7.3.0",
    "body-parser": "~1.19.0",
    "bootstrap-sass": "^3.4.1",
    "bower": "^1.8.4",
    "browserify": "^13.1.0",
    "cip-js": "git+https://github.com/CopenhagenCityArchives/cip.js.git",
    "compression": "^1.7.4",
    "connect-ensure-login": "^0.1.1",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "~1.4.1",
    "cors": "^2.7.1",
    "csurf": "^1.9.0",
    "del": "^2.2.0",
    "dotenv": "^2.0.0",
    "elasticsearch": "~14.0.0",
    "errorhandler": "~1.4.3",
    "express": "^4.17.1",
    "express-validator": "^5.3.0",
    "google-cloud": "^0.57.0",
    "google-translate": "^1.0.6",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-bower": "0.0.15",
    "gulp-clean-css": "^2.0.8",
    "gulp-concat": "^2.6.0",
    "gulp-if": "^2.0.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-print": "^2.0.1",
    "gulp-pug": "^3.0.4",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "^1.12.1",
    "gulp-svgmin": "^1.2.2",
    "gulp-svgstore": "^6.1.1",
    "gulp-uglify": "^2.0.0",
    "gulp-unique-files": "^0.2.0",
    "gulp-util": "^3.0.7",
    "hammerjs": "^2.0.8",
    "iconv": "~2.2.0",
    "jscs": "^2.10.1",
    "jshint": "^2.9.1",
    "jwt-decode": "^2.2.0",
    "keystone": "^4.1.0",
